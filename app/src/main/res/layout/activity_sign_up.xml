<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Sign.SignUpActivity"
    >

    <FrameLayout
        android:id="@+id/frameStackBackgroundSignUp"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/frameSignUp"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="start">

            <View
                android:id="@+id/viewBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/green_50" />

            <LinearLayout
                android:id="@+id/linearColumnSignUp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_gravity="top|start"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/btnCloseSignUp"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_gravity="end"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:contentDescription="@string/btn_close"
                    android:minHeight="48dp"
                    android:clickable="true"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription,ImageContrastCheck,TouchTargetSizeCheck"
                    tools:src="@drawable/img_close" />

                <FrameLayout
                    android:id="@+id/frameIllustrationSignUp"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_200pxv"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_10pxh"

                    >

                    <FrameLayout
                        android:id="@+id/frameLocationcurren"
                        android:layout_width="@dimen/_218pxh"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        tools:ignore="VisualLintBounds">

                        <FrameLayout
                            android:id="@+id/frameStackrectangle"
                            android:layout_width="@dimen/_115pxh"
                            android:layout_height="match_parent"
                            android:layout_gravity="end"
                            android:layout_marginStart="@dimen/_10pxh">

                            <ImageView
                                android:id="@+id/imageRectangle"
                                android:layout_width="@dimen/_120pxh"
                                android:layout_height="@dimen/_190pxv"
                                android:layout_gravity="start"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle_sign_up"
                                tools:ignore="ContentDescription,VisualLintBounds,ImageContrastCheck"
                                tools:src="@drawable/img_rectangle_sign_up" />

                            <ImageView
                                android:id="@+id/imageRectangleOne"
                                android:layout_width="@dimen/_78pxh"
                                android:layout_height="@dimen/_84pxh"
                                android:layout_gravity="top|end"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_57pxv"
                                android:layout_marginEnd="@dimen/_9pxh"
                                android:layout_marginBottom="@dimen/_57pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle_84x78"
                                tools:ignore="ContentDescription,ImageContrastCheck,VisualLintBounds"
                                tools:src="@drawable/img_rectangle_84x78" />
                        </FrameLayout>

                        <FrameLayout
                            android:id="@+id/frameStackrectangleTwo"
                            android:layout_width="@dimen/_218pxh"
                            android:layout_height="match_parent"
                            android:layout_gravity="start">

                            <ImageView
                                android:id="@+id/imageRectangleTwo"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="start"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle_207x218"
                                tools:ignore="ContentDescription,ImageContrastCheck,VisualLintBounds"
                                tools:src="@drawable/img_rectangle_207x218" />

                            <ImageView
                                android:id="@+id/imageRectangleThree"
                                android:layout_width="@dimen/_28pxh"
                                android:layout_height="@dimen/_55pxv"
                                android:layout_gravity="top|start"
                                android:layout_marginStart="@dimen/_44pxh"
                                android:layout_marginTop="@dimen/_60pxv"
                                android:layout_marginEnd="@dimen/_44pxh"
                                android:layout_marginBottom="@dimen/_64pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle_55x28"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_rectangle_55x28" />


                        </FrameLayout>

                    </FrameLayout>

                    <ImageView
                        android:id="@+id/imageRectangleFour"
                        android:layout_width="@dimen/_75pxh"
                        android:layout_height="@dimen/_49pxv"
                        android:layout_gravity="bottom|end"
                        android:layout_marginStart="0dp"
                        android:layout_marginEnd="@dimen/_60pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle_49x75"
                        tools:ignore="ContentDescription,ImageContrastCheck"
                        tools:src="@drawable/img_rectangle_49x75" />

                    <View
                        android:id="@+id/lineLineOne"
                        android:layout_width="@dimen/_330pxh"
                        android:layout_height="0.5dp"
                        android:layout_gravity="bottom|start"
                        android:background="@color/black_900" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtTitleSignUp"
                    style="@style/txtMontserratsemibold50Green"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_80pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:singleLine="true"
                    android:text="@string/lbl_let_s_start"
                    android:textColor="@color/foreground_color_pink"
                    tools:text="@string/lbl_let_s_start" />

                <LinearLayout
                    android:id="@+id/linearFieldUser"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/edtEmailSignUp"
                        style="@style/txtMontserratmedium14"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.1"
                        android:autofillHints=""
                        android:background="@color/trans"
                        android:hint="@string/lbl_email_address"
                        android:inputType="textEmailAddress"
                        android:minHeight="48dp"
                        android:singleLine="true"
                        android:textColorHint="@color/black"
                        tools:ignore="TextFields,TextSizeCheck,VisualLintTextFieldSize" />

                    <ImageView
                        android:id="@+id/imgUser"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:src="@drawable/img_email"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_email" />
                </LinearLayout>

                <View
                    android:id="@+id/lineVectorOne"
                    android:layout_width="@dimen/_295pxh"
                    android:layout_height="1dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:background="@color/bluegray_800" />

                <LinearLayout
                    android:id="@+id/linearFieldNameUser"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/edtUserSignUp"
                        style="@style/txtMontserratmedium14"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.1"
                        android:autofillHints=""
                        android:background="@color/trans"
                        android:hint="@string/lbl_user_name"
                        android:inputType="text"
                        android:minHeight="48dp"
                        android:singleLine="true"
                        android:textColorHint="@color/black"
                        tools:ignore="TextSizeCheck,VisualLintTextFieldSize" />

                    <ImageView
                        android:id="@+id/imgName"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_user" />
                </LinearLayout>

                <View
                    android:id="@+id/lineVectorTwo"
                    android:layout_width="@dimen/_295pxh"
                    android:layout_height="1dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:background="@color/bluegray_800" />

                <LinearLayout
                    android:id="@+id/linearField"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/edtPasswordSignUp"
                        style="@style/txtMontserratmedium14"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.1"
                        android:autofillHints=""
                        android:background="@color/trans"
                        android:hint="@string/lbl_password"
                        android:inputType="textPassword"
                        android:minHeight="48dp"
                        android:singleLine="true"
                        android:textColorHint="@color/black"
                        tools:ignore="TextSizeCheck,VisualLintTextFieldSize" />

                    <ImageView
                        android:id="@+id/imgPass"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:src="@drawable/img_lockicon"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_lockicon" />
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnSignup"
                    style="@style/btnSolidRounded"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:minHeight="48dp"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_signup"
                    android:textAllCaps="false"
                    tools:ignore="TextSizeCheck,VisualLintButtonSize"
                    tools:text="@string/lbl_signup" />

                <TextView
                    android:id="@+id/txtTermAndCondition"
                    style="@style/txtMontserratmedium12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/msg_terms_conditi"
                    android:textAlignment="center"
                    tools:ignore="TextSizeCheck,VisualLintBounds"
                    tools:text="@string/msg_terms_conditi" />
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>